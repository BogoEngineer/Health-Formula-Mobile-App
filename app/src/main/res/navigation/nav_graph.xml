        <action
            android:id="@+id/action_homeFragment_to_mySupplementPlanFragment"
            app:destination="@id/mySupplementPlanFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myMealPlanFragment"
            app:destination="@id/myMealPlanFragment" />
